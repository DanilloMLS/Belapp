<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".activities.ClienteLogadoActivity"
    tools:showIn="@layout/app_bar_cliente_logado">

    <ImageButton
        android:id="@+id/ibDepilacao"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/ibUnha"
        app:layout_constraintEnd_toStartOf="@+id/ibCabelo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/depilacao" />

    <ImageButton
        android:id="@+id/ibCabelo"

        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="6dp"
        app:layout_constraintBottom_toTopOf="@+id/ibBarba"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ibDepilacao"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/cabelo" />

    <ImageButton
        android:id="@+id/ibUnha"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toTopOf="@+id/ibSobrancelha"
        app:layout_constraintEnd_toStartOf="@+id/ibBarba"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ibDepilacao"
        app:srcCompat="@mipmap/manicure" />

    <ImageButton
        android:id="@+id/ibBarba"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toTopOf="@+id/ibOlho"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ibUnha"
        app:layout_constraintTop_toBottomOf="@+id/ibCabelo"
        app:srcCompat="@mipmap/barba" />

    <ImageButton
        android:id="@+id/ibSobrancelha"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ibOlho"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ibUnha"
        app:srcCompat="@mipmap/sombrancelhas" />

    <ImageButton
        android:id="@+id/ibOlho"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ibSobrancelha"
        app:layout_constraintTop_toBottomOf="@+id/ibBarba"
        app:srcCompat="@mipmap/maquiagem" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Depilação"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/ibDepilacao"
        app:layout_constraintEnd_toEndOf="@+id/ibDepilacao"
        app:layout_constraintStart_toStartOf="@+id/ibDepilacao"
        app:layout_constraintTop_toTopOf="@+id/ibDepilacao" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cabelo"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/ibCabelo"
        app:layout_constraintEnd_toEndOf="@+id/ibCabelo"
        app:layout_constraintStart_toStartOf="@+id/ibCabelo"
        app:layout_constraintTop_toTopOf="@+id/ibCabelo" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unhas"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/ibUnha"
        app:layout_constraintEnd_toEndOf="@+id/ibUnha"
        app:layout_constraintStart_toStartOf="@+id/ibUnha"
        app:layout_constraintTop_toTopOf="@+id/ibUnha" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Barba"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/ibBarba"
        app:layout_constraintEnd_toEndOf="@+id/ibBarba"
        app:layout_constraintStart_toStartOf="@+id/ibBarba"
        app:layout_constraintTop_toTopOf="@+id/ibBarba" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sobrancelhas"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/ibSobrancelha"
        app:layout_constraintEnd_toEndOf="@+id/ibSobrancelha"
        app:layout_constraintStart_toStartOf="@+id/ibSobrancelha"
        app:layout_constraintTop_toTopOf="@+id/ibSobrancelha" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Olhos"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/ibOlho"
        app:layout_constraintEnd_toEndOf="@+id/ibOlho"
        app:layout_constraintStart_toStartOf="@+id/ibOlho"
        app:layout_constraintTop_toTopOf="@+id/ibOlho" />

</android.support.constraint.ConstraintLayout>